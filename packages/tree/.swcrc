{
  "$schema": "https://json.schemastore.org/swcrc",
  "exclude": ["/**/*.spec.ts", "/**/*.spec.tsx", "/**/*.test.ts", "/**/*.test.tsx"],
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": false,
      "dynamicImport": false
    },
    "externalHelpers": true,
    "transform": {
      "react": {
        "runtime": "classic",
        "useSpread": true
      }
    },
    "target": "es2019"
  },
  "module": {
    "type": "es6"
  },
  "minify": false,
  "sourceMaps": true
}
